    <PackageReference Include="System.Management" Version="8.0.0" />
    <PackageReference Include="Yuu.Ini" Version="0.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="DepotDownloader">
      <HintPath>..\..\DepotDownloader\DepotDownloader\bin\Release\net8.0\DepotDownloader.dll</HintPath>
    </Reference>
    <Reference Include="TrebuchetUtils">
      <HintPath>..\..\TrebuchetGUILib\TrebuchetGUILib\bin\Release\net8.0-windows\TrebuchetUtils.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
