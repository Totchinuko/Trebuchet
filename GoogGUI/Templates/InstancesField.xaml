<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:GoogGUI"
                    xmlns:Controls="clr-namespace:GoogGUI.Controls"
                    >
    <DataTemplate x:Key="InstancesField">
        <Controls:InstancesEditor Instances="{Binding Value, Mode=TwoWay}" Profiles="{Binding App.Profiles, RelativeSource={RelativeSource AncestorType=local:MainWindow}}" />
    </DataTemplate>
</ResourceDictionary>