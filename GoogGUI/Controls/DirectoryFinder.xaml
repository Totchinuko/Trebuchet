<UserControl x:Class="GoogGUI.Controls.DirectoryFinder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GoogGUI.Controls"
             mc:Ignorable="d"
             d:DesignHeight="32" d:DesignWidth="32">
    <local:GButton
        x:Name="FindButton"
        MouseDown="FindButton_MouseDown"
        Style="{StaticResource TButton}"
        ButtonIcon="pack://application:,,,/GoogGui;component/Icons/LocalStorage.png" />
</UserControl>