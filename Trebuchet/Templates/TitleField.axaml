<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:local="clr-namespace:Trebuchet.SettingFields"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate x:Key="TitleField" DataType="local:TitleField">
        <StackPanel Orientation="Vertical" Margin="0,24,0,0">
            <TextBlock Classes="Title" Text="{Binding Name}" />
            <Border Classes="HSeparator" />
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>