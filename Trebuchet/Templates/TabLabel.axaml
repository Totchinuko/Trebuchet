<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:panels="clr-namespace:Trebuchet.ViewModels.Panels">
    <DataTemplate x:Key="TabLabel" DataType="{x:Type panels:MenuElement}">
        <StackPanel Orientation="Vertical">
            <TextBlock Text="{Binding Label}" Margin="12,6,0,6" FontWeight="SemiBold" />
            <Border Margin="0" Classes="HSeparator" />
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>