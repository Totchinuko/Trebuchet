<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:Trebuchet.Controls"
                    xmlns:settingFields="clr-namespace:Trebuchet.ViewModels.SettingFields">
    <DataTemplate x:Key="StringListFields" x:DataType="settingFields:TextListField">
        <controls:StringListEditor Values="{Binding Value, Mode=TwoWay}" />
    </DataTemplate>
</ResourceDictionary>