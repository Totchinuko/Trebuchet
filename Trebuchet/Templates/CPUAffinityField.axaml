<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:c="clr-namespace:Trebuchet.Controls"
                    xmlns:settingFields="clr-namespace:Trebuchet.ViewModels.SettingFields">
    <DataTemplate x:Key="CpuAffinityField" x:DataType="settingFields:CpuAffinityField">
        <c:FieldRowControl>
            <c:FieldRowControl.FieldContent>
                <c:CpuSelector CpuAffinity="{Binding Path=Value, Mode=TwoWay}" />
            </c:FieldRowControl.FieldContent>
        </c:FieldRowControl>
    </DataTemplate>
</ResourceDictionary>