<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:c="clr-namespace:Trebuchet.Controls"
                    xmlns:ct="clr-namespace:TrebuchetUtils.Controls;assembly=tot-gui-lib"
                    xmlns:settingFields="clr-namespace:Trebuchet.ViewModels.SettingFields">
    <DataTemplate x:Key="ComboBoxField" x:DataType="settingFields:ComboBoxField">
        <c:FieldRowControl>
            <c:FieldRowControl.FieldContent>
                <ct:ComboBox ItemsSource="{Binding Options}" SelectedIndex="{Binding Value}" MinWidth="250" MaxWidth="" HorizontalAlignment="Left" VerticalAlignment="Center" />
            </c:FieldRowControl.FieldContent>
        </c:FieldRowControl>
    </DataTemplate>
</ResourceDictionary>