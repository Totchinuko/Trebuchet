<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:vm="clr-namespace:Trebuchet.ViewModels"
                    xmlns:i="https://github.com/projektanker/icons.avalonia"
                    xmlns:a="clr-namespace:Trebuchet.Assets"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate x:Key="MixedConsolePopedOutView" x:DataType="vm:MixedConsolePopedOutViewModel">
        <Button Classes="Base Flat Sharp Sneak" Command="{Binding Close}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <i:Icon Value="mdi-open-in-app" FontSize="60" />
                <TextBlock Text="{x:Static a:Resources.Dock}" FontSize="25" />
            </StackPanel>
        </Button>
    </DataTemplate>
</ResourceDictionary>
